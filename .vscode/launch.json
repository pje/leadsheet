  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Debug tests",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-brk",
        "--allow-read",
        "${file}"
      ],
      "attachSimplePort": 9229
    },
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode_extension",
        "${workspaceFolder}"
      ]
    }
  ]
}